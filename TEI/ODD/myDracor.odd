            <!-- SCHEMA, ELEMENTS -->
            <div xml:id="div_schema">
                <head>Schema</head>
                <schemaSpec ident="myDracor" docLang="en" prefix="tei_" xml:lang="fr">
<!-- LB added list item -->
                    <moduleRef key="core"
                        include="author bibl cit desc editor emph foreign graphic head hi item l label lb lg list name note p pb publisher ref resp respStmt sp speaker stage  term title quote"/>

                    <moduleRef key="tei" except=""/>

                    <moduleRef key="header"
                        include="availability change classCode distributor fileDesc idno keywords licence listChange profileDesc publicationStmt revisionDesc sourceDesc teiHeader textClass titleStmt"/>
<!-- LB : added distributor docImprint docDate byline -->
                    <moduleRef key="textstructure"
                        include="TEI argument back body byline div dateline docAuthor docDate docImprint docTitle epigraph front text titlePage titlePart signed trailer"/>

                    <moduleRef key="drama"
                        include="actor castGroup castItem castList performance role roleDesc set spGrp"/>

                    <moduleRef key="namesdates"
                        include="event forename genName listEvent listPerson listRelation nameLink person personGrp persName relation surname"/>

                    <moduleRef key="corpus" include="particDesc"/>

                    <moduleRef key="linking" include="ab standOff"/>
<!-- :LB added table cell row -->
                    <moduleRef key="figures" include="figure table cell row"/>

                    <moduleRef key="analysis" include=""/>


